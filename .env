# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Configuration (not needed in dev mode but included for completeness)
JWT_SECRET=dev_secret_key
JWT_EXPIRES_IN=30d

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Apple App Attest (placeholder values for development)
APPLE_ROOT_CA=placeholder_base64_encoded_certificate
APPLE_TEAM_ID=your_team_id
APPLE_BUNDLE_ID=com.yourcompany.appname
